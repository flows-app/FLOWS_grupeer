{
    "id": "FLOWS/grupeer/newbalance",
    "lambda": "lambda-arn",
    "role": "iam-arn",
    "input": {
        "examples": {
            "sampleobject": {
                "account":{
                    "username":"user1",
                    "password":"password1"
                }
            }

        }
    },
    "output": {
        "examples": {
